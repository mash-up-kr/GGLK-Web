{
  "name": "GGLK-Web",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "[ -n \"$CI\" ] || lefthook install",
    "commit": "cz",
    "build": "ls-lint && react-router build",
    "dev": "ls-lint && react-router dev",
    "start": "ls-lint && react-router-serve ./build/server/index.js",
    "typecheck": "ls-lint && react-router typegen && tsc",
    "test": "ls-lint && vitest --watch",
    "ls-lint": "ls-lint"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@t3-oss/env-core": "^0.13.4",
    "@tanstack/react-query": "^5.75.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "isbot": "^5.1.27",
    "motion": "^12.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "react-router-dom": "^7.6.0",
    "tailwind-variants": "^1.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@ls-lint/ls-lint": "^2.3.0",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.13",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
